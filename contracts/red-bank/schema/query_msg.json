{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "QueryMsg",
  "anyOf": [
    {
      "type": "object",
      "required": [
        "config"
      ],
      "properties": {
        "config": {
          "type": "object"
        }
      }
    },
    {
      "type": "object",
      "required": [
        "reserve"
      ],
      "properties": {
        "reserve": {
          "type": "object",
          "required": [
            "asset"
          ],
          "properties": {
            "asset": {
              "$ref": "#/definitions/Asset"
            }
          }
        }
      }
    },
    {
      "type": "object",
      "required": [
        "reserves_list"
      ],
      "properties": {
        "reserves_list": {
          "type": "object"
        }
      }
    },
    {
      "type": "object",
      "required": [
        "debt"
      ],
      "properties": {
        "debt": {
          "type": "object",
          "required": [
            "address"
          ],
          "properties": {
            "address": {
              "$ref": "#/definitions/HumanAddr"
            }
          }
        }
      }
    },
    {
      "type": "object",
      "required": [
        "uncollateralized_loan_limit"
      ],
      "properties": {
        "uncollateralized_loan_limit": {
          "type": "object",
          "required": [
            "asset",
            "user_address"
          ],
          "properties": {
            "asset": {
              "$ref": "#/definitions/Asset"
            },
            "user_address": {
              "$ref": "#/definitions/HumanAddr"
            }
          }
        }
      }
    }
  ],
  "definitions": {
    "Asset": {
      "anyOf": [
        {
          "type": "object",
          "required": [
            "cw20"
          ],
          "properties": {
            "cw20": {
              "type": "object",
              "required": [
                "contract_addr"
              ],
              "properties": {
                "contract_addr": {
                  "$ref": "#/definitions/HumanAddr"
                }
              }
            }
          }
        },
        {
          "type": "object",
          "required": [
            "native"
          ],
          "properties": {
            "native": {
              "type": "object",
              "required": [
                "denom"
              ],
              "properties": {
                "denom": {
                  "type": "string"
                }
              }
            }
          }
        }
      ]
    },
    "HumanAddr": {
      "type": "string"
    }
  }
}
