{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ReceiveMsg",
  "anyOf": [
    {
      "description": "Redeem the sent tokens for",
      "type": "object",
      "required": [
        "redeem"
      ],
      "properties": {
        "redeem": {
          "type": "object"
        }
      }
    },
    {
      "description": "Deposit the sent cw20 tokens",
      "type": "object",
      "required": [
        "deposit_cw20"
      ],
      "properties": {
        "deposit_cw20": {
          "type": "object"
        }
      }
    },
    {
      "description": "Borrow cw20 tokens",
      "type": "object",
      "required": [
        "borrow_cw20"
      ],
      "properties": {
        "borrow_cw20": {
          "type": "object",
          "required": [
            "amount",
            "token_address"
          ],
          "properties": {
            "amount": {
              "$ref": "#/definitions/Uint256"
            },
            "token_address": {
              "$ref": "#/definitions/HumanAddr"
            }
          }
        }
      }
    },
    {
      "description": "Repay the sent cw20 tokens",
      "type": "object",
      "required": [
        "repay_cw20"
      ],
      "properties": {
        "repay_cw20": {
          "type": "object"
        }
      }
    }
  ],
  "definitions": {
    "HumanAddr": {
      "type": "string"
    },
    "Uint256": {
      "type": "string"
    }
  }
}
