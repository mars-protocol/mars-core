{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ReceiveMsg",
  "anyOf": [
    {
      "description": "Redeem the sent tokens for",
      "type": "object",
      "required": [
        "redeem"
      ],
      "properties": {
        "redeem": {
          "type": "object",
          "required": [
            "reference"
          ],
          "properties": {
            "reference": {
              "description": "Either the symbol for a terra native asset or address for a cw20 token",
              "type": "string"
            }
          }
        }
      }
    },
    {
      "description": "Deposit the sent cw20 tokens",
      "type": "object",
      "required": [
        "deposit"
      ],
      "properties": {
        "deposit": {
          "type": "object"
        }
      }
    },
    {
      "description": "Borrow cw20 tokens",
      "type": "object",
      "required": [
        "borrow"
      ],
      "properties": {
        "borrow": {
          "type": "object",
          "required": [
            "amount",
            "token_address"
          ],
          "properties": {
            "amount": {
              "$ref": "#/definitions/Uint256"
            },
            "token_address": {
              "$ref": "#/definitions/HumanAddr"
            }
          }
        }
      }
    }
  ],
  "definitions": {
    "HumanAddr": {
      "type": "string"
    },
    "Uint256": {
      "type": "string"
    }
  }
}
