{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ReceiveMsg",
  "anyOf": [
    {
      "description": "Redeem the sent tokens for the underlying asset",
      "type": "object",
      "required": [
        "redeem"
      ],
      "properties": {
        "redeem": {
          "type": "object"
        }
      }
    },
    {
      "description": "Deposit the sent cw20 tokens",
      "type": "object",
      "required": [
        "deposit_cw20"
      ],
      "properties": {
        "deposit_cw20": {
          "type": "object"
        }
      }
    },
    {
      "description": "Repay the sent cw20 tokens",
      "type": "object",
      "required": [
        "repay_cw20"
      ],
      "properties": {
        "repay_cw20": {
          "type": "object"
        }
      }
    }
  ]
}
