{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ReserveResponse",
  "type": "object",
  "required": [
    "asset_type",
    "borrow_index",
    "borrow_rate",
    "borrow_slope",
    "debt_total_scaled",
    "interests_last_updated",
    "liquidation_bonus",
    "liquidation_threshold",
    "liquidity_index",
    "liquidity_rate",
    "loan_to_value",
    "ma_token_address"
  ],
  "properties": {
    "asset_type": {
      "$ref": "#/definitions/AssetType"
    },
    "borrow_index": {
      "$ref": "#/definitions/Decimal256"
    },
    "borrow_rate": {
      "$ref": "#/definitions/Decimal256"
    },
    "borrow_slope": {
      "$ref": "#/definitions/Decimal256"
    },
    "debt_total_scaled": {
      "$ref": "#/definitions/Uint256"
    },
    "interests_last_updated": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "liquidation_bonus": {
      "$ref": "#/definitions/Decimal256"
    },
    "liquidation_threshold": {
      "$ref": "#/definitions/Decimal256"
    },
    "liquidity_index": {
      "$ref": "#/definitions/Decimal256"
    },
    "liquidity_rate": {
      "$ref": "#/definitions/Decimal256"
    },
    "loan_to_value": {
      "$ref": "#/definitions/Decimal256"
    },
    "ma_token_address": {
      "$ref": "#/definitions/HumanAddr"
    }
  },
  "definitions": {
    "AssetType": {
      "type": "string",
      "enum": [
        "cw20",
        "native"
      ]
    },
    "Decimal256": {
      "description": "A fixed-point decimal value with 18 fractional digits, i.e. Decimal256(1_000_000_000_000_000_000) == 1.0 The greatest possible value that can be represented is 115792089237316195423570985008687907853269984665640564039457.584007913129639935 (which is (2^128 - 1) / 10^18)",
      "type": "string"
    },
    "HumanAddr": {
      "type": "string"
    },
    "Uint256": {
      "type": "string"
    }
  }
}
