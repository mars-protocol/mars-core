{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "InitMsg",
  "type": "object",
  "required": [
    "cw20_code_id"
  ],
  "properties": {
    "cooldown_duration": {
      "type": [
        "integer",
        "null"
      ],
      "format": "uint64",
      "minimum": 0.0
    },
    "cw20_code_id": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "mars_token_address": {
      "anyOf": [
        {
          "$ref": "#/definitions/HumanAddr"
        },
        {
          "type": "null"
        }
      ]
    },
    "unstake_window": {
      "type": [
        "integer",
        "null"
      ],
      "format": "uint64",
      "minimum": 0.0
    }
  },
  "definitions": {
    "HumanAddr": {
      "type": "string"
    }
  }
}
